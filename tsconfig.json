{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "lib": ["dom", "esnext"],
    "strict": true,
    "types": ["on/react/types/css-prop"],
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "allowJs": true
  },
  "exclude": ["node_modules", "public", ".cache"]
}
